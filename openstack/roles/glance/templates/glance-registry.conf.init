[DEFAULT]
verbose=True
debug=False
bind_host=0.0.0.0
bind_port=9191
log_file=/var/log/glance/registry.log
use_syslog=False
log_dir=/var/log/glance
[oslo_policy]
[database]
connection=mysql://glance:{{CONFIG_GLANCE_DB_PW}}@{{os_mysql}}/glance
idle_timeout=3600
[keystone_authtoken]
identity_uri=http://{{os_keystone}}:35357
admin_tenant_name=services
admin_user=glance
admin_password={{CONFIG_GLANCE_KS_PW}}
auth_uri=http://{{os_keystone}}:5000/v2.0
[paste_deploy]
flavor=keystone
[profiler]
